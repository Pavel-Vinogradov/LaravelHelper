{
  "name": "palax/laravel-helpers",
  "description": "Basic components of laravel",
  "type": "laravel-basic-component",
  "require": {
    "php": "^8.1",
    "illuminate/support": "^10.23",
    "illuminate/console": "^10.23",
    "illuminate/database": "^10.23",
    "illuminate/filesystem": "^10.23",
    "illuminate/contracts": "^10.23",
    "illuminate/http": "^10.23",
    "laravel-zero/foundation": "^10.23",
    "symfony/http-foundation": "^6.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.3",
    "orchestra/testbench": "^8.10",
    "phpstan/phpstan": "^1.10",
    "friendsofphp/php-cs-fixer": "^3.26",
    "laravel/pint": "^1.13"
  },
  "license": "GPL-3.0-only",
  "autoload": {
    "classmap": [
      "src/"
    ],
    "psr-4": {
      "Palax\\LaravelHelpers\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Palax\\LaravelHelpers\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable"
}
